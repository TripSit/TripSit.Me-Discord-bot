{
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "eslint.format.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "jsonc",
    "json5",
    "yaml",
    "toml"
  ],
  "files.exclude": {
    ".pnpm-store": true,
    // ".devcontainer": true,
    ".github": true,
    ".vscode": false,
    // "node_modules": true,
    // ".dockerignore": true,
    // ".drone.yml": true,
    // ".eslintignore": true,
    ".eslintrc": true,
    ".gitattributes": true,
    ".gitignore": true,
    ".prettierignore": true,
    ".prettierrc": true,
    ".nvmrc": true,
    ".npmrc": true,
    ".releaserc.json": true,
    ".envExample": true,
    "coverage": true,
    // "build": true,
    "CONTRIBUTING.md": true,
    // "docker-compose.yml": true,
    // "Dockerfile": true,
    "sap-cpq.code-workspace": true,
    "azure-pipelines.yml": true,
    "package-lock.json": true,
    "pnpm-lock.yaml": true,
    "package.json": false, // This needs to be visible, otherwise the npm scripts section won't see the scripts
    "requirements.txt": true,
    "tsconfig.json": true,
    "sonar-project.properties": true,
  },
